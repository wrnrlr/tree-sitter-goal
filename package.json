{
  "name": "tree-sitter-goal",
  "version": "0.1.0",
  "description": "Goal grammar for tree-sitter",
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "parsing",
    "incremental",
    "grammar",
    "goal"
  ],
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ],
  "author": "Werner Laurensse",
  "license": "MIT",
  "scripts": {
    "test": "tree-sitter test",
    "build": "tree-sitter generate",
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip"
  },
  "dependencies": {
    "node-addon-api": "^7.1.0",
    "node-gyp-build": "^4.8.0"
  },
  "peerDependencies": {
    "tree-sitter": "^0.25.0"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {}
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.25.6",
    "prebuildify": "^6.0.0"
  },
  "tree-sitter": [
    {
      "scope": "source.goal",
      "injection-regex": "^goal$"
    }
  ]
}
